	# library(qqplotr)       # Kvantilių palyginimo (qq) diagramos.
	# library(ggrepel)       # Vienas ant kito „neužlipantys“ ggplot2 grafikų užrašai.
	# library(latex2exp)     # Lygtys ir teksto formatavimas grafikuose.
	# library(officer)       # PowerPoint, Word ir pan. dokumentų kūrimas ir grafikų išsaugojimas juose.
	# library(rvg)           # Grafikų išsaugojimui redaguojamu formatu.

	# library(data.table)    # Funkcija `fread()` tekstinių duomenų nuskaitymui.
	# library(readxl)        # Excel duomenų nuskaitymui.
	# library(openxlsx)      # Duomenų išsaugojimui į Excel.

	# library(broom)         # Statistinių modelių rezultatų vertimui į duomenų lenteles.
	# library(vcd)           # Darbui su kategoriniams duomenims (skirstiniai ir pan.).
	# library(fitdistrplus)  # Skirstinių parinkimas duomenims.

	# library(pander)        # Funkcija `pander()` rezultatų vertimui į ataskaitų lenteles.
