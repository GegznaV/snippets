	# Nustatymai -----------------------------------------------------------------

	knitr::opts_chunk\$set(
	  echo       = TRUE,   # Ar rodyti programos kodą ataskaitoje?
	  fig.height = 3,      # Numatytasis paveikslo aukštis
	  fig.width  = 5,      # Numatytasis paveikslo plotis
	  fig.align  = "center"
	)

	Sys.setlocale(locale = "${3:`r if (.Platform$OS.type == "windows" && getRversion() < "4.2") "Lithuanian" else "lt_LT.UTF-8"`}")
	options(scipen = 8)   # Rodomi bent 8 skaitmenys prieš verčiant skaičių į inžinerinį formatą (pvz., 1.2E9).
	theme_set(theme_bw()) # Numatytoji ggplot2 tema

